package xyzProblems;

import java.util.Scanner;

public class Uc1 {
public static void main(String[] args) {
	//var
	int  x1, x2, y1, y2;
	double F;
	Scanner sc = new Scanner(System.in);
	
	System.out.println("enter values");
	x1 = sc.nextInt();
	x2 = sc.nextInt();
	y1 = sc.nextInt();
	y2 = sc.nextInt();
	
	F = Math.sqrt((x2-x1)*(x2-x1) + (y2-y1)*(y2-y1));
	System.out.println("length"+F);
}
}

